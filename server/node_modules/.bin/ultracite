#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pedroaba/projects/c216-L1-task-manager/server/node_modules/.pnpm/ultracite@5.4.7_@types+node@24.6.2_tsx@4.20.6_typescript@5.9.3_yaml@2.8.1/node_modules/ultracite/dist/node_modules:/Users/pedroaba/projects/c216-L1-task-manager/server/node_modules/.pnpm/ultracite@5.4.7_@types+node@24.6.2_tsx@4.20.6_typescript@5.9.3_yaml@2.8.1/node_modules/ultracite/node_modules:/Users/pedroaba/projects/c216-L1-task-manager/server/node_modules/.pnpm/ultracite@5.4.7_@types+node@24.6.2_tsx@4.20.6_typescript@5.9.3_yaml@2.8.1/node_modules:/Users/pedroaba/projects/c216-L1-task-manager/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pedroaba/projects/c216-L1-task-manager/server/node_modules/.pnpm/ultracite@5.4.7_@types+node@24.6.2_tsx@4.20.6_typescript@5.9.3_yaml@2.8.1/node_modules/ultracite/dist/node_modules:/Users/pedroaba/projects/c216-L1-task-manager/server/node_modules/.pnpm/ultracite@5.4.7_@types+node@24.6.2_tsx@4.20.6_typescript@5.9.3_yaml@2.8.1/node_modules/ultracite/node_modules:/Users/pedroaba/projects/c216-L1-task-manager/server/node_modules/.pnpm/ultracite@5.4.7_@types+node@24.6.2_tsx@4.20.6_typescript@5.9.3_yaml@2.8.1/node_modules:/Users/pedroaba/projects/c216-L1-task-manager/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ultracite@5.4.7_@types+node@24.6.2_tsx@4.20.6_typescript@5.9.3_yaml@2.8.1/node_modules/ultracite/dist/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/ultracite@5.4.7_@types+node@24.6.2_tsx@4.20.6_typescript@5.9.3_yaml@2.8.1/node_modules/ultracite/dist/index.js" "$@"
fi
